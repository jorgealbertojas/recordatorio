<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:id="@+id/nRelative"
    android:layout_height="match_parent"
    xmlns:card_view="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/main_fundo"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:alpha="0.2"
        android:scaleType="centerCrop"
        android:src="@drawable/salada" />

    <ImageView
        android:id="@+id/main_icon_schedule_image_view3"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_centerHorizontal="true"
        android:scaleType="centerCrop"
        android:src="@mipmap/ic_icon"
        card_view:layout_constraintBottom_toTopOf="@+id/criar"
        card_view:layout_constraintEnd_toEndOf="parent"
        card_view:layout_constraintStart_toStartOf="parent"
        card_view:srcCompat="@mipmap/ic_icon" />

    <ImageView
        android:id="@+id/main_icon_schedule_image_view"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_centerInParent="true"
        android:src="@mipmap/icones150px01"
        card_view:layout_constraintBottom_toBottomOf="parent"
        card_view:layout_constraintEnd_toEndOf="parent"
        card_view:layout_constraintStart_toStartOf="parent"
        card_view:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/criar"
        android:layout_width="200dp"
        android:layout_height="160dp"

        android:layout_centerInParent="true"
        android:background="#0000"
        android:paddingTop="110dp"
        android:text="Iniciar"
        android:textColor="@color/colorRedStart"
        android:textSize="17sp"
        android:textStyle="normal"
        card_view:layout_constraintBottom_toBottomOf="parent"
        card_view:layout_constraintEnd_toEndOf="parent"
        card_view:layout_constraintStart_toStartOf="parent"
        card_view:layout_constraintTop_toTopOf="parent" />



    <ImageView
        android:id="@+id/imageviewEnviar"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_centerInParent="true"
        android:src="@mipmap/icones150px02"
        android:visibility="invisible"
        card_view:layout_constraintBottom_toBottomOf="parent"
        card_view:layout_constraintEnd_toEndOf="parent"
        card_view:layout_constraintStart_toStartOf="parent"
        card_view:layout_constraintTop_toTopOf="parent" />


    <com.getbase.floatingactionbutton.FloatingActionsMenu
        android:id="@+id/multiple_actions_down"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="10dp"
        android:focusableInTouchMode="true"
        android:visibility="visible"
        card_view:layout_constraintBottom_toBottomOf="parent"
        card_view:layout_constraintEnd_toEndOf="parent"
        tools:fab_addButtonColorNormal="@color/color_primary"
        tools:fab_addButtonColorPressed="@color/white_pressed"
        tools:fab_addButtonPlusIconColor="@color/color_accent"
        card_view:fab_addButtonColorNormal="@color/color_accent"
        tools:fab_addButtonSize="normal"
        tools:fab_expandDirection="up"
        tools:fab_labelStyle="@style/menu_labels_style">


        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/gps"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            card_view:fab_icon="@mipmap/mapa"
            card_view:fab_title="Gps"
            tools:fab_colorNormal="@color/white"
            tools:fab_colorPressed="@color/white_pressed"
            tools:fab_size="mini" />


        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/chat"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            card_view:fab_icon="@mipmap/chat"
            card_view:fab_title="Chat"
            tools:fab_colorNormal="@color/white"
            tools:fab_colorPressed="@color/white_pressed"
            tools:fab_size="mini" />


        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/bkp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            card_view:fab_icon="@mipmap/database"
            tools:fab_colorNormal="@color/white"
            tools:fab_colorPressed="@color/white_pressed" />


        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/buscar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            card_view:fab_icon="@mipmap/configuration"
            tools:fab_colorNormal="@color/white"
            tools:fab_colorPressed="@color/white_pressed"
            tools:fab_title="Set bottom menu enabled/disabled" />


    </com.getbase.floatingactionbutton.FloatingActionsMenu>
    </androidx.constraintlayout.widget.ConstraintLayout>