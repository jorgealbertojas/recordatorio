<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <androidx.cardview.widget.CardView
        xmlns:card_view="http://schemas.android.com/apk/res-auto"
        android:orientation="vertical"
        android:layout_width="fill_parent"
        card_view:cardElevation="10dp"
        card_view:cardCornerRadius="1dp"
        android:layout_height="240dp"
        android:clickable="true"
        android:focusable="true"
        android:foreground="?android:attr/selectableItemBackground"
        android:tag="0"
        android:visibility="visible"
        android:id="@+id/cardView1"
        android:layout_marginTop="0dp"
        android:paddingTop="5dp"
        android:paddingRight="5dp"
        android:paddingLeft="5dp">
    <RelativeLayout
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dip"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/TextViewLOG"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="unknown"
            android:layout_centerHorizontal="true"
            android:textColor="@color/blue_dark"
            android:textSize="30dip" >
        </TextView>

        <TextView
            android:id="@+id/TextViewLAT"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/TextViewLOG"
            android:textColor="@color/blue_dark"
            android:text="unknown"
            android:textSize="30dip" >
        </TextView>

        <TextView
            android:id="@+id/TextView06"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:textColor="@color/blue_dark"
            android:layout_below="@+id/TextViewLAT"
            android:text="unknown"
            android:textSize="30dip" >
        </TextView>

        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:id="@+id/imageButtonConecao"
            android:layout_centerHorizontal="true"
            android:layout_gravity="center"
            android:src="@mipmap/aceitar"
            android:background="@null"
            android:layout_below="@+id/TextView06"
   />

        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:id="@+id/imageViewGPS"
            android:src="@mipmap/gps"
 />
    </RelativeLayout>


    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        xmlns:card_view="http://schemas.android.com/apk/res-auto"
        android:orientation="vertical"
        android:layout_width="fill_parent"
        card_view:cardElevation="10dp"
        card_view:cardCornerRadius="1dp"
        android:layout_height="120dp"
        android:clickable="true"
        android:focusable="true"
        android:foreground="?android:attr/selectableItemBackground"
        android:tag="0"
        android:visibility="visible"
        android:id="@+id/cardView2"
        android:paddingTop="5dp"
        android:paddingRight="5dp"
        android:paddingLeft="5dp"
        android:layout_below="@+id/cardView1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">
    <RelativeLayout
        android:id="@+id/linearLayout5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <TextView
            android:id="@+id/TextViewID"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="unknown"
            android:layout_centerHorizontal="true"
            android:textColor="@color/blue_dark"
            android:textSize="30dip" >
        </TextView>

        <TextView
        android:id="@+id/TextViewNOME"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="unknown"
            android:layout_below="@+id/TextViewID"
        android:textColor="@color/blue_dark"
        android:textSize="30dip"
        android:layout_centerHorizontal="true">
    </TextView>

        <ImageView
            android:layout_width="60dip"
            android:layout_height="60dip"
            android:id="@+id/imageViewUSER"
            android:src="@mipmap/count"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />
    </RelativeLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        xmlns:card_view="http://schemas.android.com/apk/res-auto"
        android:orientation="vertical"
        android:layout_width="fill_parent"
        card_view:cardElevation="10dp"
        card_view:cardCornerRadius="1dp"
        android:layout_height="120dp"
        android:clickable="true"
        android:focusable="true"
        android:foreground="?android:attr/selectableItemBackground"
        android:tag="0"
        android:visibility="visible"
        android:id="@+id/cardView3"
        android:paddingTop="5dp"
        android:paddingRight="5dp"
        android:paddingLeft="5dp"
        android:layout_below="@+id/cardView2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">
        <RelativeLayout
            android:id="@+id/linearLayout6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true">

            <TextView
                android:id="@+id/TextViewDATA"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="unknown"
                android:layout_centerHorizontal="true"
                android:textColor="@color/blue_dark"
                android:textSize="30dip" >
            </TextView>

            <TextView
                android:id="@+id/TextViewHORA"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="unknown"
                android:layout_below="@+id/TextViewDATA"
                android:textColor="@color/blue_dark"
                android:textSize="30dip"
                android:layout_centerHorizontal="true">
            </TextView>

            <ImageView
                android:layout_width="60dip"
                android:layout_height="60dip"
                android:id="@+id/imageViewTIME"
                android:src="@mipmap/time"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
        </RelativeLayout>

    </androidx.cardview.widget.CardView>


    <ImageButton
        android:layout_width="60dip"
        android:layout_height="60dip"
        android:id="@+id/imageButtonClose"
        android:layout_gravity="center_horizontal"
        android:src="@mipmap/ok"
        android:background="@android:color/transparent"
        android:scaleType="fitCenter"
        android:layout_marginBottom="38dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

</RelativeLayout>